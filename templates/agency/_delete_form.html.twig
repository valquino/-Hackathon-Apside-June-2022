<form method="post" action="{{ path('app_agency_delete', {'id': agency.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ agency.id) }}">
    <button class="btn btn-danger"><i class="bi bi-trash3"></i></button>
</form>
